        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "byjoby/flatrr": "^1.3",
        "erusev/parsedown-extra": "^0.8.1",
        "twig/twig": "~2",
        "scssphp/scssphp": "~1",
        "imagine/imagine": "~1",
        "chrisjean/php-ico": "~1",
        "tedivm/stash": "0.14.*",
        "symfony/yaml": "~5.0||~4.0",
        "matthiasmullie/minify": "~1",
        "monolog/monolog": "^2.0",
        "composer-plugin-api": "^1.1",
        "symfony/filesystem": "~5.0||~4.0||~3.0||~2.1"
    },
    "autoload": {
        "psr-4": {
            "Leafcutter\\": "src"
        }
    },
    "type": "composer-plugin",
    "extra": {
        "class": "Leafcutter\\Composer\\Plugin"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests",
        "test-win": "./vendor/bin/phpunit tests"
    }
}
